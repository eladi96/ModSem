############################
#   Object Properties
############################

# Object Property: :characterIn (personaggio in)

AnnotationAssertion(rdfs:comment :characterIn "A relation between a character and the programme in which appears."@en)
AnnotationAssertion(rdfs:label :characterIn "personaggio in"@it)
ObjectPropertyDomain(:characterIn :Character)
ObjectPropertyRange(:characterIn po:Programme)

# Object Property: :dubbedBy (doppiato da)

AnnotationAssertion(rdfs:comment :dubbedBy "Relation between a character and the voice actor that dubs it."@en)
AnnotationAssertion(rdfs:label :dubbedBy "doppiato da"@it)
InverseObjectProperties(:dubbedBy :voiceOf)
ObjectPropertyDomain(:dubbedBy :Character)
ObjectPropertyRange(:dubbedBy :VoiceActor)

# Object Property: :endingOf (sigla finale di)

AnnotationAssertion(rdfs:comment :endingOf "A relation between an ending theme and its programme."@en)
AnnotationAssertion(rdfs:label :endingOf "sigla finale di"@it)
InverseObjectProperties(:endingOf :hasEnding)
ObjectPropertyDomain(:endingOf :Ending)
ObjectPropertyRange(:endingOf po:Programme)

# Object Property: :followsEpisode (segue episodio)

AnnotationAssertion(rdfs:comment :followsEpisode "A relation between an episode and the antecedent."@en)
AnnotationAssertion(rdfs:label :followsEpisode "segue episodio"@it)
SubObjectPropertyOf(:followsEpisode <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows>)
InverseObjectProperties(:followsEpisode :precedesEpisode)
TransitiveObjectProperty(:followsEpisode)
ObjectPropertyDomain(:followsEpisode po:Episode)
ObjectPropertyRange(:followsEpisode po:Episode)

# Object Property: :hasEnding (ha sigla finale)

AnnotationAssertion(rdfs:comment :hasEnding "A relation between a programme and its ending theme."@it)
AnnotationAssertion(rdfs:label :hasEnding "ha sigla finale"@it)
FunctionalObjectProperty(:hasEnding)
ObjectPropertyDomain(:hasEnding po:Programme)
ObjectPropertyRange(:hasEnding :Ending)

# Object Property: :hasEpisode (ha episodio)

AnnotationAssertion(rdfs:comment :hasEpisode "A relation between a series and the episodes that are part of it."@en)
AnnotationAssertion(rdfs:label :hasEpisode "ha episodio"@it)
SubObjectPropertyOf(:hasEpisode <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>)
InverseObjectProperties(:hasEpisode :isEpisodeOf)
ObjectPropertyDomain(:hasEpisode po:Series)
ObjectPropertyRange(:hasEpisode po:Episode)

# Object Property: :hasGenre (ha genere)

AnnotationAssertion(rdfs:comment :hasGenre "Relation between a programme and the programme's genres"@en)
AnnotationAssertion(rdfs:label :hasGenre "ha genere"@it)
ObjectPropertyDomain(:hasGenre po:Programme)
ObjectPropertyRange(:hasGenre po:Genre)

# Object Property: :hasOpening (ha sigla iniziale)

AnnotationAssertion(rdfs:comment :hasOpening "A relation between a programme and its opening theme."@en)
AnnotationAssertion(rdfs:label :hasOpening "ha sigla iniziale"@it)
InverseObjectProperties(:hasOpening :openingOf)
FunctionalObjectProperty(:hasOpening)
ObjectPropertyDomain(:hasOpening po:Programme)
ObjectPropertyRange(:hasOpening :Opening)

# Object Property: :isEpisodeOf (episodio di)

AnnotationAssertion(rdfs:comment :isEpisodeOf "A relation between an episode and the series which is part of."@en)
AnnotationAssertion(rdfs:label :isEpisodeOf "episodio di"@it)
SubObjectPropertyOf(:isEpisodeOf <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf>)
FunctionalObjectProperty(:isEpisodeOf)
ObjectPropertyDomain(:isEpisodeOf po:Episode)
ObjectPropertyRange(:isEpisodeOf po:Series)

# Object Property: :narrativeRoleIn (ruolo narrativo in)

AnnotationAssertion(rdfs:comment :narrativeRoleIn "Relation between a narrative role and the programme which is part of."@en)
AnnotationAssertion(rdfs:label :narrativeRoleIn "ruolo narrativo in"@it)
SubObjectPropertyOf(:narrativeRoleIn :roleIn)
ObjectPropertyDomain(:narrativeRoleIn :NarrativeRole)
ObjectPropertyRange(:narrativeRoleIn po:Programme)

# Object Property: :openingOf (sigla iniziale di)

AnnotationAssertion(rdfs:comment :openingOf "A relation between an opening theme and its programme."@en)
AnnotationAssertion(rdfs:label :openingOf "sigla iniziale di"@it)
ObjectPropertyDomain(:openingOf :Opening)
ObjectPropertyRange(:openingOf po:Programme)

# Object Property: :precedesEpisode (precede episodio)

AnnotationAssertion(rdfs:comment :precedesEpisode "Relation between an episode and the successive."@en)
AnnotationAssertion(rdfs:label :precedesEpisode "precede episodio"@it)
SubObjectPropertyOf(:precedesEpisode <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes>)
TransitiveObjectProperty(:precedesEpisode)
ObjectPropertyDomain(:precedesEpisode po:Episode)
ObjectPropertyRange(:precedesEpisode po:Episode)

# Object Property: :produced (ha prodotto)

AnnotationAssertion(rdfs:comment :produced "Relation between a studio and the programme that it produced."@en)
AnnotationAssertion(rdfs:label :produced "ha prodotto"@it)
InverseObjectProperties(:produced :producedBy)
ObjectPropertyDomain(:produced :Studio)
ObjectPropertyRange(:produced po:Programme)

# Object Property: :producedBy (prodotto da)

AnnotationAssertion(rdfs:comment :producedBy "A relation between a programme and the studio that produced it."@en)
AnnotationAssertion(rdfs:label :producedBy "prodotto da"@it)
ObjectPropertyDomain(:producedBy po:Programme)
ObjectPropertyRange(:producedBy :Studio)

# Object Property: :productionRoleIn (ruolo di produzione in)

AnnotationAssertion(rdfs:label :productionRoleIn "Relation between a production role and the programme whose production has worked on."@en)
AnnotationAssertion(rdfs:label :productionRoleIn "ruolo di produzione in"@it)
SubObjectPropertyOf(:productionRoleIn :roleIn)
ObjectPropertyDomain(:productionRoleIn :ProductionRole)
ObjectPropertyRange(:productionRoleIn po:Programme)

# Object Property: :roleIn (ruolo in)

AnnotationAssertion(rdfs:comment :roleIn "A relation between a role and the programme in which operates."@en)
AnnotationAssertion(rdfs:label :roleIn "ruolo in"@it)
ObjectPropertyDomain(:roleIn <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role>)
ObjectPropertyRange(:roleIn po:Programme)

# Object Property: :voiceActorIn (doppiatore in)

AnnotationAssertion(rdfs:comment :voiceActorIn "A relation between a real person and a programme for which performs the role of voice actor."@en)
AnnotationAssertion(rdfs:label :voiceActorIn "doppiatore in"@it)
ObjectPropertyDomain(:voiceActorIn :VoiceActor)
ObjectPropertyRange(:voiceActorIn po:Programme)

# Object Property: :voiceOf (voce di)

AnnotationAssertion(rdfs:comment :voiceOf "Relation between a voice actor and the character that interprets."@en)
AnnotationAssertion(rdfs:label :voiceOf "voce di"@it)
ObjectPropertyDomain(:voiceOf :VoiceActor)
ObjectPropertyRange(:voiceOf :Character)

# Object Property: :voiceRole (ruolo doppiaggio)

AnnotationAssertion(rdfs:comment :voiceRole "A relation between the role of Voice Actor and a Character."@en)
AnnotationAssertion(rdfs:label :voiceRole "ruolo doppiaggio"@it)
ObjectPropertyDomain(:voiceRole :VoiceActorRole)
ObjectPropertyRange(:voiceRole :Character)


############################
#   Data Properties
############################

# Data Property: :englishTitle (titolo inglese)

AnnotationAssertion(rdfs:comment :englishTitle "The english title of the programme."@en)
AnnotationAssertion(rdfs:label :englishTitle "titolo inglese"@it)
DataPropertyDomain(:englishTitle po:Programme)
DataPropertyRange(:englishTitle xsd:string)

# Data Property: :episodeTitle (titolo episodio)

AnnotationAssertion(rdfs:comment :episodeTitle "The title of an episode."@en)
AnnotationAssertion(rdfs:label :episodeTitle "titolo episodio"@it)
DataPropertyDomain(:episodeTitle po:Episode)
DataPropertyRange(:episodeTitle xsd:string)

# Data Property: :firstAiring (prima visione)

AnnotationAssertion(rdfs:comment :firstAiring "The date of the first airing."@en)
AnnotationAssertion(rdfs:label :firstAiring "prima visione"@it)
DataPropertyDomain(:firstAiring po:Programme)
DataPropertyRange(:firstAiring xsd:dateTime)

# Data Property: :runtime (durata)

AnnotationAssertion(rdfs:comment :runtime "The duration of the programme in minutes."@en)
AnnotationAssertion(rdfs:label :runtime "durata"@it)
SubDataPropertyOf(:runtime owl:topDataProperty)
DataPropertyDomain(:runtime po:Programme)
DataPropertyRange(:runtime xsd:nonNegativeInteger)



############################
#   Classes
############################

# Class: po:Category (Categoria)

AnnotationAssertion(rdfs:comment po:Category "A category provides a way of classifying a set of programmes. Such classifications can be performed according to multiple dimensions and taxonomies, e.g. genre, format, places, people, subjects..."@en)
AnnotationAssertion(rdfs:label po:Category "Categoria"@it)

# Class: po:Episode (Episodio)

AnnotationAssertion(rdfs:comment po:Episode "A particular episode."@en)
AnnotationAssertion(rdfs:label po:Episode "Episodio"@it)
SubClassOf(po:Episode po:Programme)
SubClassOf(po:Episode <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem>)
DisjointClasses(po:Episode po:Series)

# Class: po:Genre (Genere)

AnnotationAssertion(rdfs:comment po:Genre "An anchor point for a programmes' genre taxonomy, e.g. 'Drama'/'Biographical'."@en)
AnnotationAssertion(rdfs:label po:Genre "Genere"@it)
EquivalentClasses(po:Genre ObjectOneOf(:Action :Adventure :Comedy :Drama :Fantasy :Mystery :Romance :School :Sci_Fi :Slice_Of_Life))
SubClassOf(po:Genre po:Category)

# Class: po:Programme (Programma)

AnnotationAssertion(rdfs:comment po:Programme "A programme, can either be a brand, a series or an episode"@en)
AnnotationAssertion(rdfs:label po:Programme "Programma"@it)

# Class: po:Series (Serie)

AnnotationAssertion(rdfs:comment po:Series "A series"@en)
AnnotationAssertion(rdfs:label po:Series "Serie"@it)
EquivalentClasses(po:Series ObjectIntersectionOf(po:Programme ObjectMinCardinality(2 :hasEpisode po:Episode)))
SubClassOf(po:Series po:Programme)
SubClassOf(po:Series <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List>)

# Class: :Character (Personaggio)

AnnotationAssertion(rdfs:label :Character "Personaggio"@it)
EquivalentClasses(:Character ObjectIntersectionOf(:ImaginaryPerson ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :NarrativeRole)))
EquivalentClasses(:Character ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :NarrativeRole))
SubClassOf(:Character <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent>)

# Class: :Director (Regista)

AnnotationAssertion(rdfs:comment :Director "A person that has the role of director."@en)
AnnotationAssertion(rdfs:label :Director "Regista"@it)
EquivalentClasses(:Director ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :DirectorRole))
SubClassOf(:Director :RealPerson)

# Class: :DirectorRole (Ruolo Regista)

AnnotationAssertion(rdfs:comment :DirectorRole "The role of who oversees the making of the work."@en)
AnnotationAssertion(rdfs:label :DirectorRole "Ruolo Regista"@it)
SubClassOf(:DirectorRole :ProductionRole)

# Class: :Ending (Sigla Finale)

AnnotationAssertion(rdfs:comment :Ending "The theme song at the ending of a programme."@en)
AnnotationAssertion(rdfs:label :Ending "Sigla Finale"@it)
SubClassOf(:Ending :ThemeSong)
DisjointClasses(:Ending :Opening)

# Class: :ImaginaryPerson (Persona Immaginaria)

AnnotationAssertion(rdfs:comment :ImaginaryPerson "A person that doesn't exist in the real world."@en)
AnnotationAssertion(rdfs:label :ImaginaryPerson "Persona Immaginaria"@it)
SubClassOf(:ImaginaryPerson foaf:Person)

# Class: :Main (Principale)

AnnotationAssertion(rdfs:comment :Main "A main character"@en)
AnnotationAssertion(rdfs:label :Main "Principale"@it)
SubClassOf(:Main :NarrativeRole)
DisjointClasses(:Main :Supporting)
DisjointClasses(:Main :Villain)

# Class: :MainCharacter (Personaggio Principale)

AnnotationAssertion(rdfs:comment :MainCharacter "A character that has a main role."@en)
AnnotationAssertion(rdfs:label :MainCharacter "Personaggio Principale"@it)
EquivalentClasses(:MainCharacter ObjectIntersectionOf(:Character ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :Main)))
SubClassOf(:MainCharacter :Character)

# Class: :Movie (Lungometraggio)

AnnotationAssertion(rdfs:comment :Movie "A film with a running time long enough to be considered the principal or sole film to fill a program."@en)
AnnotationAssertion(rdfs:label :Movie "Lungometraggio"@it)
EquivalentClasses(:Movie ObjectIntersectionOf(po:Programme DataSomeValuesFrom(:runtime DatatypeRestriction(xsd:nonNegativeInteger xsd:minInclusive "60"^^xsd:nonNegativeInteger))))
SubClassOf(:Movie po:Programme)

# Class: :NarrativeRole (Ruolo Narrativo)

AnnotationAssertion(rdfs:comment :NarrativeRole "A role of a character in a fiction work"@en)
AnnotationAssertion(rdfs:label :NarrativeRole "Ruolo Narrativo"@it)
SubClassOf(:NarrativeRole <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role>)

# Class: :Opening (Sigla Iniziale)

AnnotationAssertion(rdfs:comment :Opening "The theme song at the start of a programme."@en)
AnnotationAssertion(rdfs:label :Opening "Sigla Iniziale"@it)
SubClassOf(:Opening :ThemeSong)

# Class: :ProductionRole (Ruolo di Produzione)

AnnotationAssertion(rdfs:comment :ProductionRole "A role involved in the production of a programmme"@en)
AnnotationAssertion(rdfs:label :ProductionRole "Ruolo di Produzione"@it)
SubClassOf(:ProductionRole <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role>)

# Class: :RealPerson (Persona Reale)

AnnotationAssertion(rdfs:comment :RealPerson "A person that exists, or existed, in the real world"@en)
AnnotationAssertion(rdfs:label :RealPerson "Persona Reale"@it)
SubClassOf(:RealPerson foaf:Person)

# Class: :SoundDirector (Direttore del Suono)

AnnotationAssertion(rdfs:comment :SoundDirector "A person that has the role of the sound director."@en)
AnnotationAssertion(rdfs:label :SoundDirector "Direttore del Suono"@it)
EquivalentClasses(:SoundDirector ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :SoundDirectorRole))
SubClassOf(:SoundDirector :RealPerson)

# Class: :SoundDirectorRole (Ruolo Direttore del Suono)

AnnotationAssertion(rdfs:comment :SoundDirectorRole "The role of the head of the sound department and the person responsible for planning the audiography."@en)
AnnotationAssertion(rdfs:label :SoundDirectorRole "Ruolo Direttore del Suono"@it)
SubClassOf(:SoundDirectorRole :ProductionRole)

# Class: :Studio (Studio)

AnnotationAssertion(rdfs:comment :Studio "A production studio."@en)
AnnotationAssertion(rdfs:label :Studio "Studio"@it)

# Class: :Supporting (Supporto)

AnnotationAssertion(rdfs:comment :Supporting "A side character"@en)
AnnotationAssertion(rdfs:label :Supporting "Supporto"@it)
SubClassOf(:Supporting :NarrativeRole)
DisjointClasses(:Supporting :Villain)

# Class: :SupportingCharacter (Personaggio di Supporto)

AnnotationAssertion(rdfs:comment :SupportingCharacter "A character that has a supporting role."@en)
AnnotationAssertion(rdfs:label :SupportingCharacter "Personaggio di Supporto"@it)
EquivalentClasses(:SupportingCharacter ObjectIntersectionOf(:Character ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :Supporting)))
SubClassOf(:SupportingCharacter :Character)

# Class: :ThemeSong (Sigla)

AnnotationAssertion(rdfs:comment :ThemeSong "A piece of music from a television programme, film, etc. that is remembered as the music that represents that programme or film."@en)
AnnotationAssertion(rdfs:label :ThemeSong "Sigla"@it)

# Class: :Villain (Cattivo)

AnnotationAssertion(rdfs:comment :Villain "A bad character"@en)
AnnotationAssertion(rdfs:label :Villain "Cattivo"@it)
SubClassOf(:Villain :NarrativeRole)

# Class: :VillainCharacter (Personaggio Cattivo)

AnnotationAssertion(rdfs:comment :VillainCharacter "A character that has the role of villain."@en)
AnnotationAssertion(rdfs:label :VillainCharacter "Personaggio Cattivo"@it)
EquivalentClasses(:VillainCharacter ObjectIntersectionOf(:Character ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :Villain)))
SubClassOf(:VillainCharacter :Character)

# Class: :VoiceActor (Doppiatore)

AnnotationAssertion(rdfs:comment :VoiceActor "A person that has the role of voice acting."@en)
AnnotationAssertion(rdfs:label :VoiceActor "Doppiatore"@it)
EquivalentClasses(:VoiceActor ObjectSomeValuesFrom(<http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole> :VoiceActorRole))
SubClassOf(:VoiceActor :RealPerson)

# Class: :VoiceActorRole (Ruolo Doppiatore)

AnnotationAssertion(rdfs:comment :VoiceActorRole "A role whose job is to provide the voices for characters in animated films."@en)
AnnotationAssertion(rdfs:label :VoiceActorRole "Ruolo Doppiatore"@it)
SubClassOf(:VoiceActorRole :ProductionRole)

# Class: foaf:Person (Persona)

AnnotationAssertion(rdfs:comment foaf:Person "The Person class represents people. Something is a Person if it is a person. We don't nitpic about whether they're alive, dead, real, or imaginary."@en)
AnnotationAssertion(rdfs:label foaf:Person "Persona"@it)
SubClassOf(foaf:Person <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent>)


############################
#   Named Individuals
############################

# Individual: :Action (Azione)

AnnotationAssertion(rdfs:comment :Action "A genre in which the protagonist or protagonists are thrust into a series of events that typically include violence, extended fighting, physical feats and frantic chases"@en)
AnnotationAssertion(rdfs:label :Action "Azione"@it)

# Individual: :Adventure (Avventura)

