<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/eladi/ontologies/anime-ontology#"
     xml:base="http://www.semanticweb.org/eladi/ontologies/anime-ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:po="http://purl.org/ontology/po/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:list="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:objectrole="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/list.owl"/>
        <dc:creator>Enrico Ladisa</dc:creator>
        <dc:date>2020-08-20</dc:date>
        <dc:description xml:lang="it">Ontologia per la rappresentazione e la catalogazione delle opere di animazione giapponesi, quali film, serie di animazione e cortometraggi.</dc:description>
        <dc:title>Anime Ontology</dc:title>
        <vann:preferredNamespacePrefix>anon</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>http://www.semanticweb.org/eladi/ontologies/anime-ontology#</vann:preferredNamespaceUri>
        <rdfs:label>Anime Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#characterIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#characterIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Character"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#narrativeRoleIn"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">A relation between a character and the programme in which appears.</rdfs:comment>
        <rdfs:label xml:lang="it">personaggio in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#costarring -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#costarring">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#MainCharacter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#MainCharacter"/>
        <rdfs:comment xml:lang="en">A relation between two main characters.</rdfs:comment>
        <rdfs:label xml:lang="it">coprotagonista</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#directedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#directedBy">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Director"/>
        <rdfs:comment xml:lang="en">A relation between a programme and its director.</rdfs:comment>
        <rdfs:label xml:lang="it">diretto da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#dubbedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#dubbedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#VoiceActor"/>
        <rdfs:comment xml:lang="en">Relation between a character and the voice actor that dubs it.</rdfs:comment>
        <rdfs:label xml:lang="it">doppiato da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#endingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#endingOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasEnding"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Ending"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:comment xml:lang="en">A relation between an ending theme and its programme.</rdfs:comment>
        <rdfs:label xml:lang="it">sigla finale di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#followsEpisode -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#followsEpisode">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#precedesEpisode"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:comment xml:lang="en">A relation between an episode and the antecedent.</rdfs:comment>
        <rdfs:label xml:lang="it">segue episodio</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasEnding -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasEnding">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Ending"/>
        <rdfs:comment xml:lang="it">A relation between a programme and its ending theme.</rdfs:comment>
        <rdfs:label xml:lang="it">ha sigla finale</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasEpisode -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasEpisode">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#isEpisodeOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Series"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:comment xml:lang="en">A relation between a series and the episodes that are part of it.</rdfs:comment>
        <rdfs:label xml:lang="it">ha episodio</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasGenre -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasGenre">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Genre"/>
        <rdfs:comment xml:lang="en">Relation between a programme and the programme&apos;s genres</rdfs:comment>
        <rdfs:label xml:lang="it">ha genere</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasOpening -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasOpening">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#openingOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Opening"/>
        <rdfs:comment xml:lang="en">A relation between a programme and its opening theme.</rdfs:comment>
        <rdfs:label xml:lang="it">ha sigla iniziale</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#isEpisodeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#isEpisodeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/bag.owl#itemOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Series"/>
        <rdfs:comment xml:lang="en">A relation between an episode and the series which is part of.</rdfs:comment>
        <rdfs:label xml:lang="it">episodio di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#mainOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#mainOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#MainCharacter"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:comment xml:lang="en">Relation between a character and the programme in which has a main role.</rdfs:comment>
        <rdfs:label xml:lang="it">protagonista di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#narrativeRoleIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#narrativeRoleIn">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#roleIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#NarrativeRole"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:comment xml:lang="en">Relation between a narrative role and the programme which is part of.</rdfs:comment>
        <rdfs:label xml:lang="it">ruolo narrativo in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#openingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#openingOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Opening"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:comment xml:lang="en">A relation between an opening theme and its programme.</rdfs:comment>
        <rdfs:label xml:lang="it">sigla iniziale di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#precedesEpisode -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#precedesEpisode">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:comment xml:lang="en">Relation between an episode and the successive.</rdfs:comment>
        <rdfs:label xml:lang="it">precede episodio</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#produced -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#produced">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#producedBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Studio"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:comment xml:lang="en">Relation between a studio and the programme that it produced.</rdfs:comment>
        <rdfs:label xml:lang="it">ha prodotto</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#producedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#producedBy">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Studio"/>
        <rdfs:comment xml:lang="en">A relation between a programme and the studio that produced it.</rdfs:comment>
        <rdfs:label xml:lang="it">prodotto da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#productionRoleIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#productionRoleIn">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#roleIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#ProductionRole"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:comment xml:lang="en">Relation between a production role and the programme whose production has worked on.</rdfs:comment>
        <rdfs:label xml:lang="it">ruolo di produzione in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#roleIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#roleIn">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#Role"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:comment xml:lang="en">A relation between a role and the programme in which operates.</rdfs:comment>
        <rdfs:label xml:lang="it">ruolo in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceActorIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceActorIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#VoiceActor"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/po/Programme"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceRole"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#characterIn"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">A relation between a real person and a programme for which performs the role of voice actor.</rdfs:comment>
        <rdfs:label xml:lang="it">doppiatore in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#VoiceActor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Character"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#hasRole"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceRole"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Relation between a voice actor and the character that interprets.</rdfs:comment>
        <rdfs:label xml:lang="it">voce di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceRole -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceRole">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#VoiceActorRole"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Character"/>
        <rdfs:comment xml:lang="en">A relation between the role of Voice Actor and a Character.</rdfs:comment>
        <rdfs:label xml:lang="it">ruolo doppiaggio</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#workedFor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#workedFor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#RealPerson"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/eladi/ontologies/anime-ontology#Studio"/>
        <rdfs:comment xml:lang="en">Relation between a person and a production studio for which he/she woorked for in the making of a programme.</rdfs:comment>
        <rdfs:label xml:lang="it">ha lavorato per</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#englishTitle -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#englishTitle">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The english title of the programme.</rdfs:comment>
        <rdfs:label xml:lang="it">titolo inglese</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#episodeTitle -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#episodeTitle">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Episode"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The title of an episode.</rdfs:comment>
        <rdfs:label xml:lang="it">titolo episodio</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#firstAiring -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#firstAiring">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">The date of the first airing.</rdfs:comment>
        <rdfs:label xml:lang="it">prima visione</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#runtime -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#runtime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="en">The duration of the programme in minutes.</rdfs:comment>
        <rdfs:label xml:lang="it">durata</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/eladi/ontologies/anime-ontology#shortSeries -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/eladi/ontologies/anime-ontology#shortSeries">
        <rdfs:domain rdf:resource="http://purl.org/ontology/po/Programme"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Boolean value that is true if the series has less then 15 episodes.</rdfs:comment>
        <rdfs:label xml:lang="it">serie breve</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/po/Category -->

    <owl:Class rdf:about="http://purl.org/ontology/po/Category">
        <rdfs:comment xml:lang="en">A category provides a way of classifying a set of programmes. Such classifications can be performed according to multiple dimensions and taxonomies, e.g. genre, format, places, people, subjects...</rdfs:comment>
        <rdfs:label xml:lang="it">Categoria</rdfs:label>
