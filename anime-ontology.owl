@prefix : <http://www.semanticweb.org/eladi/ontologies/anime-ontology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix po: <http://purl.org/ontology/po/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix list: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix objectrole: <http://www.ontologydesignpatterns.org/cp/owl/objectrole.owl#> .
@base <http://www.semanticweb.org/eladi/ontologies/anime-ontology> .

<http://www.semanticweb.org/eladi/ontologies/anime-ontology> rdf:type owl:Ontology ;
                                                              owl:imports <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl> ,
                                                                          <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                              dc:creator "Enrico Ladisa" ;
                                                              dc:date "2020-08-20" ;
                                                              dc:description "Ontologia per la rappresentazione e la catalogazione delle opere di animazione giapponesi, quali film, serie di animazione e cortometraggi."@it ;
                                                              dc:title "Anime Ontology" ;
                                                              vann:preferredNamespacePrefix "anon" ;
                                                              vann:preferredNamespaceUri "http://www.semanticweb.org/eladi/ontologies/anime-ontology#" ;
                                                              rdfs:label "Anime Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#characterIn
:characterIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Character ;
             rdfs:range po:Programme ;
             owl:propertyChainAxiom ( objectrole:hasRole
                                      :narrativeRoleIn
                                    ) ;
             rdfs:comment "A relation between a character and the programme in which appears."@en ;
             rdfs:label "personaggio in"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#costarring
:costarring rdf:type owl:ObjectProperty ,
                     owl:SymmetricProperty ;
            rdfs:domain :MainCharacter ;
            rdfs:range :MainCharacter ;
            rdfs:comment "A relation between two main characters."@en ;
            rdfs:label "coprotagonista"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#directedBy
:directedBy rdf:type owl:ObjectProperty ;
            rdfs:domain po:Programme ;
            rdfs:range :Director ;
            rdfs:comment "A relation between a programme and its director."@en ;
            rdfs:label "diretto da"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#dubbedBy
:dubbedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :voiceOf ;
          rdfs:domain :Character ;
          rdfs:range :VoiceActor ;
          rdfs:comment "Relation between a character and the voice actor that dubs it."@en ;
          rdfs:label "doppiato da"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#endingOf
:endingOf rdf:type owl:ObjectProperty ;
          owl:inverseOf :hasEnding ;
          rdfs:domain :Ending ;
          rdfs:range po:Programme ;
          rdfs:comment "A relation between an ending theme and its programme."@en ;
          rdfs:label "sigla finale di"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#followsEpisode
:followsEpisode rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows> ;
                owl:inverseOf :precedesEpisode ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain po:Episode ;
                rdfs:range po:Episode ;
                rdfs:comment "A relation between an episode and the antecedent."@en ;
                rdfs:label "segue episodio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasEnding
:hasEnding rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain po:Programme ;
           rdfs:range :Ending ;
           rdfs:comment "A relation between a programme and its ending theme."@it ;
           rdfs:label "ha sigla finale"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasEpisode
:hasEpisode rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf list:hasItem ;
            owl:inverseOf :isEpisodeOf ;
            rdfs:domain po:Series ;
            rdfs:range po:Episode ;
            rdfs:comment "A relation between a series and the episodes that are part of it."@en ;
            rdfs:label "ha episodio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasGenre
:hasGenre rdf:type owl:ObjectProperty ;
          rdfs:domain po:Programme ;
          rdfs:range po:Genre ;
          rdfs:comment "Relation between a programme and the programme's genres"@en ;
          rdfs:label "ha genere"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#hasOpening
:hasOpening rdf:type owl:ObjectProperty ;
            owl:inverseOf :openingOf ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain po:Programme ;
            rdfs:range :Opening ;
            rdfs:comment "A relation between a programme and its opening theme."@en ;
            rdfs:label "ha sigla iniziale"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#isEpisodeOf
:isEpisodeOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf list:itemOf ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain po:Episode ;
             rdfs:range po:Series ;
             rdfs:comment "A relation between an episode and the series which is part of."@en ;
             rdfs:label "episodio di"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#mainOf
:mainOf rdf:type owl:ObjectProperty ;
        rdfs:domain :MainCharacter ;
        rdfs:range po:Programme ;
        rdfs:comment "Relation between a character and the programme in which has a main role."@en ;
        rdfs:label "protagonista di"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#narrativeRoleIn
:narrativeRoleIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :roleIn ;
                 rdfs:domain :NarrativeRole ;
                 rdfs:range po:Programme ;
                 rdfs:comment "Relation between a narrative role and the programme which is part of."@en ;
                 rdfs:label "ruolo narrativo in"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#openingOf
:openingOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Opening ;
           rdfs:range po:Programme ;
           rdfs:comment "A relation between an opening theme and its programme."@en ;
           rdfs:label "sigla iniziale di"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#precedesEpisode
:precedesEpisode rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes> ;
                 rdf:type owl:TransitiveProperty ;
                 rdfs:domain po:Episode ;
                 rdfs:range po:Episode ;
                 rdfs:comment "Relation between an episode and the successive."@en ;
                 rdfs:label "precede episodio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#produced
:produced rdf:type owl:ObjectProperty ;
          owl:inverseOf :producedBy ;
          rdfs:domain :Studio ;
          rdfs:range po:Programme ;
          rdfs:comment "Relation between a studio and the programme that it produced."@en ;
          rdfs:label "ha prodotto"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#producedBy
:producedBy rdf:type owl:ObjectProperty ;
            rdfs:domain po:Programme ;
            rdfs:range :Studio ;
            rdfs:comment "A relation between a programme and the studio that produced it."@en ;
            rdfs:label "prodotto da"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#productionRoleIn
:productionRoleIn rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :roleIn ;
                  rdfs:domain :ProductionRole ;
                  rdfs:range po:Programme ;
                  rdfs:comment "Relation between a production role and the programme whose production has worked on."@en ;
                  rdfs:label "ruolo di produzione in"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#roleIn
:roleIn rdf:type owl:ObjectProperty ;
        rdfs:domain objectrole:Role ;
        rdfs:range po:Programme ;
        rdfs:comment "A relation between a role and the programme in which operates."@en ;
        rdfs:label "ruolo in"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceActorIn
:voiceActorIn rdf:type owl:ObjectProperty ;
              rdfs:domain :VoiceActor ;
              rdfs:range po:Programme ;
              owl:propertyChainAxiom ( objectrole:hasRole
                                       :voiceRole
                                       :characterIn
                                     ) ;
              rdfs:comment "A relation between a real person and a programme for which performs the role of voice actor."@en ;
              rdfs:label "doppiatore in"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceOf
:voiceOf rdf:type owl:ObjectProperty ;
         rdfs:domain :VoiceActor ;
         rdfs:range :Character ;
         owl:propertyChainAxiom ( objectrole:hasRole
                                  :voiceRole
                                ) ;
         rdfs:comment "Relation between a voice actor and the character that interprets."@en ;
         rdfs:label "voce di"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#voiceRole
:voiceRole rdf:type owl:ObjectProperty ;
           rdfs:domain :VoiceActorRole ;
           rdfs:range :Character ;
           rdfs:comment "A relation between the role of Voice Actor and a Character."@en ;
           rdfs:label "ruolo doppiaggio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#workedFor
:workedFor rdf:type owl:ObjectProperty ;
           rdfs:domain :RealPerson ;
           rdfs:range :Studio ;
           rdfs:comment "Relation between a person and a production studio for which he/she woorked for in the making of a programme."@en ;
           rdfs:label "ha lavorato per"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#englishTitle
:englishTitle rdf:type owl:DatatypeProperty ;
              rdfs:domain po:Programme ;
              rdfs:range xsd:string ;
              rdfs:comment "The english title of the programme."@en ;
              rdfs:label "titolo inglese"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#episodeTitle
:episodeTitle rdf:type owl:DatatypeProperty ;
              rdfs:domain po:Episode ;
              rdfs:range xsd:string ;
              rdfs:comment "The title of an episode."@en ;
              rdfs:label "titolo episodio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#firstAiring
:firstAiring rdf:type owl:DatatypeProperty ;
             rdfs:domain po:Programme ;
             rdfs:range xsd:dateTime ;
             rdfs:comment "The date of the first airing."@en ;
             rdfs:label "prima visione"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#runtime
:runtime rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain po:Programme ;
         rdfs:range xsd:nonNegativeInteger ;
         rdfs:comment "The duration of the programme in minutes."@en ;
         rdfs:label "durata"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#shortSeries
:shortSeries rdf:type owl:DatatypeProperty ;
             rdfs:domain po:Programme ;
             rdfs:range xsd:boolean ;
             rdfs:comment "Boolean value that is true if the series has less then 15 episodes."@en ;
             rdfs:label "serie breve"@it .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ontology/po/Category
po:Category rdf:type owl:Class ;
            rdfs:comment "A category provides a way of classifying a set of programmes. Such classifications can be performed according to multiple dimensions and taxonomies, e.g. genre, format, places, people, subjects..."@en ;
            rdfs:label "Categoria"@it .


###  http://purl.org/ontology/po/Episode
po:Episode rdf:type owl:Class ;
           rdfs:subClassOf po:Programme ,
                           <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
           owl:disjointWith po:Series ;
           rdfs:comment "A particular episode."@en ;
           rdfs:label "Episodio"@it .


###  http://purl.org/ontology/po/Genre
po:Genre rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Action
                                           :Adventure
                                           :Comedy
                                           :Drama
                                           :Fantasy
                                           :Mystery
                                           :Romance
                                           :School
                                           :Sci_Fi
                                           :Slice_Of_Life
                                         )
                             ] ;
         rdfs:subClassOf po:Category ;
         rdfs:comment "An anchor point for a programmes' genre taxonomy, e.g. 'Drama'/'Biographical'."@en ;
         rdfs:label "Genere"@it .


###  http://purl.org/ontology/po/Programme
po:Programme rdf:type owl:Class ;
             rdfs:comment "A programme, can either be a brand, a series or an episode"@en ;
             rdfs:label "Programma"@it .


###  http://purl.org/ontology/po/Series
po:Series rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( po:Programme
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :hasEpisode ;
                                                       owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                       owl:onClass po:Episode
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf po:Programme ,
                          <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
          rdfs:comment "A series"@en ;
          rdfs:label "Serie"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Character
:Character rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :ImaginaryPerson
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty objectrole:hasRole ;
                                                        owl:someValuesFrom :NarrativeRole
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty objectrole:hasRole ;
                                 owl:someValuesFrom :NarrativeRole
                               ] ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent> ;
           rdfs:label "Personaggio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Director
:Director rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty objectrole:hasRole ;
                                owl:someValuesFrom :DirectorRole
                              ] ;
          rdfs:subClassOf :RealPerson ;
          rdfs:comment "A person that has the role of director."@en ;
          rdfs:label "Regista"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#DirectorRole
:DirectorRole rdf:type owl:Class ;
              rdfs:subClassOf :ProductionRole ;
              rdfs:comment "The role of who oversees the making of the work."@en ;
              rdfs:label "Ruolo Regista"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Ending
:Ending rdf:type owl:Class ;
        rdfs:subClassOf :ThemeSong ;
        owl:disjointWith :Opening ;
        rdfs:comment "The theme song at the ending of a programme."@en ;
        rdfs:label "Sigla Finale"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#ImaginaryPerson
:ImaginaryPerson rdf:type owl:Class ;
                 rdfs:subClassOf foaf:Person ;
                 rdfs:comment "A person that doesn't exist in the real world."@en ;
                 rdfs:label "Persona Immaginaria"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Main
:Main rdf:type owl:Class ;
      rdfs:subClassOf :NarrativeRole ;
      owl:disjointWith :Supporting ,
                       :Villain ;
      rdfs:comment "A main character"@en ;
      rdfs:label "Principale"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#MainCharacter
:MainCharacter rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :Character
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty objectrole:hasRole ;
                                                            owl:someValuesFrom :Main
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf :Character ;
               rdfs:comment "A character that has a main role."@en ;
               rdfs:label "Personaggio Principale"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Movie
:Movie rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( po:Programme
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :runtime ;
                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                         owl:onDatatype xsd:nonNegativeInteger ;
                                                                         owl:withRestrictions ( [ xsd:minInclusive "60"^^xsd:nonNegativeInteger
                                                                                                ]
                                                                                              )
                                                                       ]
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf po:Programme ;
       rdfs:comment "A film with a running time long enough to be considered the principal or sole film to fill a program."@en ;
       rdfs:label "Lungometraggio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#NarrativeRole
:NarrativeRole rdf:type owl:Class ;
               rdfs:subClassOf objectrole:Role ;
               rdfs:comment "A role of a character in a fiction work"@en ;
               rdfs:label "Ruolo Narrativo"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Opening
:Opening rdf:type owl:Class ;
         rdfs:subClassOf :ThemeSong ;
         rdfs:comment "The theme song at the start of a programme."@en ;
         rdfs:label "Sigla Iniziale"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#ProductionRole
:ProductionRole rdf:type owl:Class ;
                rdfs:subClassOf objectrole:Role ;
                rdfs:comment "A role involved in the production of a programmme"@en ;
                rdfs:label "Ruolo di Produzione"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#RealPerson
:RealPerson rdf:type owl:Class ;
            rdfs:subClassOf foaf:Person ;
            rdfs:comment "A person that exists, or existed, in the real world"@en ;
            rdfs:label "Persona Reale"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#SoundDirector
:SoundDirector rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty objectrole:hasRole ;
                                     owl:someValuesFrom :SoundDirectorRole
                                   ] ;
               rdfs:subClassOf :RealPerson ;
               rdfs:comment "A person that has the role of the sound director."@en ;
               rdfs:label "Direttore del Suono"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#SoundDirectorRole
:SoundDirectorRole rdf:type owl:Class ;
                   rdfs:subClassOf :ProductionRole ;
                   rdfs:comment "The role of the head of the sound department and the person responsible for planning the audiography."@en ;
                   rdfs:label "Ruolo Direttore del Suono"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Studio
:Studio rdf:type owl:Class ;
        rdfs:comment "A production studio."@en ;
        rdfs:label "Studio"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Supporting
:Supporting rdf:type owl:Class ;
            rdfs:subClassOf :NarrativeRole ;
            owl:disjointWith :Villain ;
            rdfs:comment "A side character"@en ;
            rdfs:label "Supporto"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#SupportingCharacter
:SupportingCharacter rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :Character
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty objectrole:hasRole ;
                                                                  owl:someValuesFrom :Supporting
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Character ;
                     rdfs:comment "A character that has a supporting role."@en ;
                     rdfs:label "Personaggio di Supporto"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#ThemeSong
:ThemeSong rdf:type owl:Class ;
           rdfs:comment "A piece of music from a television programme, film, etc. that is remembered as the music that represents that programme or film."@en ;
           rdfs:label "Sigla"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Villain
:Villain rdf:type owl:Class ;
         rdfs:subClassOf :NarrativeRole ;
         rdfs:comment "A bad character"@en ;
         rdfs:label "Cattivo"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#VillainCharacter
:VillainCharacter rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Character
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty objectrole:hasRole ;
                                                               owl:someValuesFrom :Villain
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Character ;
                  rdfs:comment "A character that has the role of villain."@en ;
                  rdfs:label "Personaggio Cattivo"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#VoiceActor
:VoiceActor rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty objectrole:hasRole ;
                                  owl:someValuesFrom :VoiceActorRole
                                ] ;
            rdfs:subClassOf :RealPerson ;
            rdfs:comment "A person that has the role of voice acting."@en ;
            rdfs:label "Doppiatore"@it .


###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#VoiceActorRole
:VoiceActorRole rdf:type owl:Class ;
                rdfs:subClassOf :ProductionRole ;
                rdfs:comment "A role whose job is to provide the voices for characters in animated films."@en ;
                rdfs:label "Ruolo Doppiatore"@it .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#Agent> ;
            rdfs:comment "The Person class represents people. Something is a Person if it is a person. We don't nitpic about whether they're alive, dead, real, or imaginary."@en ;
            rdfs:label "Persona"@it .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/eladi/ontologies/anime-ontology#Action
:Action rdf:type owl:NamedIndividual ;
        rdfs:comment "A genre in which the protagonist or protagonists are thrust into a series of events that typically include violence, extended fighting, physical feats and frantic chases"@en ;
